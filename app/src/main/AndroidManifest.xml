<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techtitudetribe.yummy">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Yummy">
        <activity android:name=".MyAddressActivity"></activity>
        <activity android:name=".OrderConfirmationActivity" />
        <activity android:name=".EmailSignupActivity" />
        <activity android:name=".MenuItemActivity" />
        <activity android:name=".CartItemDescriptionActivity" />
        <activity android:name=".MyOrderDetailActivity" />
        <activity android:name=".MyOrdersActivity" />
        <activity android:name=".OtpVerificationActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>